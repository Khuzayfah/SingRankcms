{
  "name": "singrank",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:netlify": "node scripts/fix-blog-netlify.js && node scripts/prepare-blog-for-netlify.js && node scripts/ensure-blog-dirs.js && next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:skipcheck": "next build --no-lint",
    "start": "next start",
    "start:win": "powershell -File scripts/run-start.ps1",
    "start:win:3001": "powershell -File scripts/run-start.ps1 -Port 3001",
    "start:win:3002": "powershell -File scripts/run-start.ps1 -Port 3002",
    "lint": "next lint",
    "clean": "rimraf .next",
    "dev:clean": "npm run clean && npm run dev",
    "dev:win": "powershell -File scripts/run-dev.ps1",
    "build:clean": "npm run clean && npm run build",
    "analyze": "cross-env ANALYZE=true next build",
    "predev": "npm run clean",
    "postbuild": "next-sitemap",
    "type-check": "tsc --noEmit",
    "setup-blog": "node scripts/setup-blog.js",
    "debug:blog": "node scripts/debug-blog-paths.js",
    "deploy": "npm run build:netlify && netlify deploy --prod",
    "fix-blog-paths": "node scripts/fix-blog-paths.js",
    "ensure-blog-dirs": "node scripts/ensure-blog-dirs.js",
    "prebuild:netlify": "npm run fix-blog-paths",
    "lighthouse": "cross-env ANALYZE=false NODE_ENV=production next build && next start",
    "pwa:setup": "node scripts/generate-pwa-icons.js",
    "pwa:convert-icons": "node scripts/convert-icons-to-png.js",
    "pwa:prepare": "npm run pwa:setup && npm run pwa:convert-icons",
    "optimize:js": "node scripts/optimize-js.js",
    "optimize:dom": "node scripts/optimize-dom.js",
    "optimize": "npm run optimize:js && npm run optimize:dom",
    "killport:3000": "powershell -Command \"Get-Process -Id (Get-NetTCPConnection -LocalPort 3000).OwningProcess | Stop-Process -Force\"",
    "fix-blog": "node scripts/fix-blog-netlify.js"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.3.1",
    "@heroicons/react": "2.2.0",
    "@nivo/bar": "0.88.0",
    "@nivo/core": "0.88.0",
    "@nivo/geo": "0.88.0",
    "@nivo/line": "0.88.0",
    "@nivo/pie": "0.88.0",
    "@radix-ui/react-dialog": "1.1.6",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "d3": "7.9.0",
    "decap-cms-app": "3.6.2",
    "firebase-admin": "13.2.0",
    "framer-motion": "11.18.2",
    "fs-extra": "11.3.0",
    "glob": "11.0.1",
    "gray-matter": "4.0.3",
    "lucide-react": "0.477.0",
    "next": "14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "5.0.0",
    "react-icons": "5.5.0",
    "react-tsparticles": "2.12.2",
    "recharts": "2.15.1",
    "remark": "15.0.1",
    "remark-html": "16.0.1",
    "schema-dts": "1.1.5",
    "tailwind-merge": "3.0.2",
    "tailwindcss-animate": "1.0.7",
    "tsparticles": "3.8.1",
    "tsparticles-slim": "2.12.0",
    "typescript": "^5.3.3",
    "typewriter-effect": "2.21.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "5.9.4",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "critters": "0.0.23",
    "cross-env": "7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "10.0.2",
    "next-sitemap": "4.2.3",
    "postcss": "^8.4.33",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "tailwindcss": "^3.4.1",
    "webpack-bundle-analyzer": "4.10.2"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
